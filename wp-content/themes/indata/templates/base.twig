{% block html_head_container %}

{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}
<body class="{{body_class}}" data-template="base.twig">

    <!--page start-->
    <div class="page">

        <!-- preloader start -->
        <div id="preloader">
          <div id="status">&nbsp;</div>
        </div>
        <!-- preloader end -->

        <!--header start-->
        <header id="masthead" class="header ttm-header-style-01">
            <!-- ttm-topbar-wrapper -->
            <div class="ttm-topbar-wrapper clearfix">
                <div class="container">
                    <div class="ttm-topbar-content">
                        <ul class="top-contact text-left">
                            <li><i class="fa fa-map-marker"></i>{{add_info.address}}</li>{# {% endif %} #}
                            <li><i class="fa fa-envelope-o"></i><a href="mailto:{{add_info.email}}">{{add_info.email}}</a></li>
                        
                        </ul>
                        <div class="topbar-right text-right">
                            <ul class="top-contact">
                                <li><i class="fa fa-clock-o"></i>{{add_info.office_hours}}</li>
                            </ul>
                            <div class="ttm-social-links-wrapper list-inline">
                                <ul class="social-icons">
                                    <li><a href="{{add_info.facebook_link}}" target="_blank" class=" tooltip-bottom" data-tooltip="Facebook"><i class="fa fa-facebook"></i></a>
                                    </li>
                                    <li><a href="{{add_info.linkedin_link}}" target="_blank" class=" tooltip-bottom" data-tooltip="Linkedin"><i class="fa fa-linkedin"></i></a>
                                    </li>
                                     <li><a href="{{add_info.instagram_link}}" target="_blank" class=" tooltip-bottom" data-tooltip="Instagram"><i class="fa fa-instagram" aria-hidden="true"></i></a>
                                    </li>
                                </ul>
                            </div>
                            {# <div class="header-btn">
                                <a class="ttm-btn ttm-btn-size-md  ttm-btn-bgcolor-skincolor" href="{{site.theme.link}}/assets/#">Get A Quote</a>
                            </div> #}
                        </div>
                    </div>
                </div>
            </div><!-- ttm-topbar-wrapper end -->
            <!-- ttm-header-wrap -->
            <div class="ttm-header-wrap">
                <!-- ttm-stickable-header-w -->
                <div id="ttm-stickable-header-w" class="ttm-stickable-header-w clearfix">
                    <div id="site-header-menu" class="site-header-menu">
                        <div class="site-header-menu-inner ttm-stickable-header">
                            <div class="container">
                                <!-- site-branding -->
                                <div class="site-branding">
                                    <a class="home-link" href="{{site.link}}" title="Indata Consulting" rel="home">
                                    	{# <h4 style="margin-top:35px;">Indata Consulting services</h4> #}
                                        <img id="logo-img" class="img-center" src="{{site.theme.link}}/assets/images/logo1.png" alt="logo">
                                    </a>
                                </div><!-- site-branding end -->
                                <!--site-navigation -->
                                <div id="site-navigation" class="site-navigation">
                                    <div class="ttm-menu-toggle">
                                        <input type="checkbox" id="menu-toggle-form" />
                                        <label for="menu-toggle-form" class="ttm-menu-toggle-block">
                                            <span class="toggle-block toggle-blocks-1"></span>
                                            <span class="toggle-block toggle-blocks-2"></span>
                                            <span class="toggle-block toggle-blocks-3"></span>
                                        </label>
                                    </div>
                                    <nav id="menu" class="menu">
                                        <ul class="dropdown">
                                          {% for menu in top_menu.get_items %}
                                             {% if menu.children %}
                                           <li class="{{ menu.classes|join(' ') }}"><a href="{{menu.get_link}}">{{menu.name}}</a>
                                            
                                       <ul>
                                        {% for sub_menu in menu.children %}
                                            <li class=""><a href="{{sub_menu.get_link}}">{{sub_menu.name}}</a>
                                           {% if sub_menu.children %}
                                            <ul>
                                                {% for child in sub_menu.children %}
                                                <li class=""><a href="{{child.get_link}}">{{child.name}}</a></li>

                                            {% endfor %}
                                            </ul>
                                            {% endif %}
                                        </li>
                                        {% endfor %}
                                        </ul>
                                    </li>
                                    {% else %}
                                        <li class="{{ menu.classes|join(' ') }}"><a href="{{menu.get_link}}">{{menu.name}}</a></li>
                                    {% endif %}

                                    {% endfor %}
                                </ul>
                                
                           
                                             
                                        </ul>
                                    </nav>
                                </div><!-- site-navigation end-->
                            </div>
                        </div>
                    </div>
                </div><!-- ttm-stickable-header-w end-->
            </div><!--ttm-header-wrap end -->

        </header><!--header end-->

        {# <rs-module-wrap id="rev_slider_1_1_wrapper" data-source="gallery"> #}
        
             {% block content %}
                Sorry, no content
             {% endblock %}
     
         {# </rs-module-wrap> #}

       
{% block footer %}
	{% include 'footer.twig' %}
	   
{% endblock %}
	</body>
</html>
